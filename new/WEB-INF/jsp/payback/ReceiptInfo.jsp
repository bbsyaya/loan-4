<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    <%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
     <div title="借据信息" style="padding:20px;">
     <table>
     <tr>
     <td>*借据编号:</td>
     <td><input type="text" class="insertSignReceiptId" readonly= "true " name="insertSignReceiptId"/></td>
     <td>*业务渠道:</td>
     <td><input type="text" class="insertSignBusinessChannel" readonly= "true " name="insertSignBusinessChannel"/></td>
     </tr>
     <tr>
     <td>*客户名称:</td>
     <td><input type="text" class="insertSignCustName" readonly= "true " name="insertSignCustName"/></td>
     <td>*商户名称:</td>
     <td><input type="text" class="insertSignPosCustName" readonly= "true " name="insertSignPosCustName"/></td>
     </tr>
     <tr>
     <td>*贷款金额:</td>
     <td><input type="text" class="insertSignLoanAmount" readonly= "true " name="insertSignLoanAmount"/></td>
     <td>*贷款利率:</td>
     <td><input type="text" class="insertSignLoanInterest" readonly= "true " name="insertSignLoanInterest"/></td>
     </tr>
     <tr>
     <td>*起贷日:</td>
     <td><input type="text" class="insertSignBeginDate" readonly= "true " name="insertSignBeginDate"/></td>
     <td>*到期日:</td>
     <td><input type="text" class="insertSignEndDate" readonly= "true " name="insertSignEndDate"/></td>
     </tr>
     <tr>
     <td>还款方式:</td>
     <td><input type="text" class="insertSignPaybackWay" readonly= "true " name="insertSignPaybackWay"/></td>
     <td>*贷款用途:</td>
     <td><input type="text" class="insertSignLoanUsage" readonly= "true " name="insertSignLoanUsage"/></td>
     </tr>
     <tr>
     <td>银行账号:</td>
     <td><input type="text" class="insertSignPaybackAccount" readonly= "true " name="insertSignPaybackAccount"/></td>
     <td>*账户开户行:</td>
     <td><input type="text" class="insertSignAccountOpenBank" readonly= "true " name="insertSignAccountOpenBank"/></td>
     </tr>
     <tr>
     <td>*账户分行:</td>
     <td><input type="text" class="insertSignAccountBranchBank" readonly= "true " name="insertSignAccountBranchBank"/></td>
     <td>*账户支行:</td>
     <td><input type="text" class="insertSignAccountSubBranchBank" readonly= "true " name="insertSignAccountSubBranchBank"/></td>
     </tr>
     <tr>
     <td>*贷款总余额:</td>
     <td><input type="text" class="insertSignLoanTotalBalance" readonly= "true " name="insertSignLoanTotalBalance"/></td>
     <td>*正常余额:</td>
     <td><input type="text" class="insertSignNormalBalance" readonly= "true " name="insertSignNormalBalance"/></td>
     </tr>
     <tr>
     <td>*逾期余额:</td>
     <td><input type="text" class="insertSignOverdueBalance" readonly= "true " name="insertSignOverdueBalance"/></td>
     <td>*表内欠息:</td>
     <td><input type="text" class="insertSignInnerOwnedInterest" readonly= "true " name="insertSignInnerOwnedInterest"/></td>
     </tr>
     <tr>
     <td>*表外 欠息:</td>
     <td><input type="text" class="insertSignOutterOwnedInterest" readonly= "true " name="insertSignOutterOwnedInterest"/></td>
     </tr>
     </table>
     </div>