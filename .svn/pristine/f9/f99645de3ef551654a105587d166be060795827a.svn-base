<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    <%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
    <div title="客户基本信息" style="padding:20px;">
                	<table>
                		<tr>
                		<td>*客户编号:</td>
                		<td><input type="text" class="insertSignCustId" readonly= "true " name="insertSignCustId3" value="${customer.custId}"></td>
                		<td>*客户名称:</td>
                		<td><input type="text" class="insertSignCustName" readonly= "true " name="insertSignCustName2" value="${customer.custName}" ></td>
                		</tr>
                		<tr>
                		<td>*证件类型:</td>
                		<td><input type="text" class="insertSignCredentialtype" readonly= "true " name="insertSignCredentialtype" value="${customer.paperKind}"></td>
                		<td>*证件号码:</td>
                		<td><input type="text" class="insertSignCredentialNo" readonly= "true " name="insertSignCredentialNo" value="${customer.paperId}"></td>
                		</tr>
                		<tr>
                		<td>*出生日期:</td>
                		<td><input type="text" class="insertSignBirthDate" readonly= "true " name="insertSignBirthDate" value="${customer.birtDate}"></td>
                		<td>*性别:</td>
                		<td><input type="text" class="insertSignGender" readonly= "true " name="insertSignGender" value="${customer.sexSign}"></td>
                		</tr>
                		<tr>
                		<td>*最高学历:</td>
                		<td><input type="text" class="insertSignHighestDegree" readonly= "true " name="insertSignHighestDegree" value="${customer.educSign}"></td>
                		<td>*婚姻状况:</td>
                		<td><input type="text" class="insertSignMarriageStatus" readonly= "true " name="insertSignMarriageStatus" value="${customer.marrSign}"></td>
                		</tr>
                		<tr>
                		<td>*居住地址-省:</td>
						<td><input type="text" class="insertSignProvince" readonly= "true " name="insertSignProvince" value="${customer.residentProv}"></td>
                		<td>*居住地址-市:</td>
                		<td><input type="text" class="insertSignCity" readonly= "true " name="insertSignCity" value="${customer.residentCity}"></td>
                		<tr>
                		<td>*居住地址-区:</td>
                		<td><input type="text" class="insertSignDivision" readonly= "true " name="insertSignDivision" value="${customer.residentDivision}"></td>
                		<td>*居住地址-具体</td>
                		<td><input type="text" class="insertSignDetailedAddress" readonly= "true " name="insertSignCurrentAddress" value="${customer.residentDetail}"></td>
                		</tr>
                		<tr>
                		<td>*子女人数:</td>
                		<td><input type="text" class="insertSignChildrenNo" readonly= "true " name="insertSignChildrenNo" value="${customer.childNum}"></td>
                		<td>*电子邮件:</td>
                		<td><input type="text" class="insertSignEmailAddress" readonly= "true " name="insertSignEmailAddress" value="${customer.email}"></td> 		
                		</tr>
                		<tr>
                		<td>*手机号码:</td>
                		<td><input type="text" class="insertSignCellphoneNo" readonly= "true " name="insertSignCellphoneNo" value="${customer.mobilePhone}"></td>
                		<td>*办公电话:</td>
                		<td><input type="text" class="insertSignOfficeTel" readonly= "true " name="insertSignOfficeTel" value="${customer.tel}"></td>
                		</tr>
                		<tr>
                		<td>*微信号:</td>
                		<td><input type="text" class="insertSignWechatNo" readonly= "true " name="insertSignWechatNo" value="${customer.weixinNo}"></td>
                		<td>*QQ号:</td>
                		<td><input type="text" class="insertSignQQNo" readonly= "true " name="insertSignQQNo" value="${customer.qqNo}"></td>
                		</tr>
                	</table>
                </div>