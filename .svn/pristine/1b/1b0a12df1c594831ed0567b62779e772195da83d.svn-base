<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    <%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
     <div title="借据信息" style="padding:20px;">
     <table>
     <tr>
     <td>*借据编号:</td>
     <td><input type="text" class="insertSignReceiptId" readonly= "true " name="insertSignReceiptId" value="${receipt.receiptId}"/></td>
     <td>*业务渠道:</td>
     <td><input type="text" class="insertSignBusinessChannel" readonly= "true " name="insertSignBusinessChannel" value="${receipt.channel}"/></td>
     </tr>
     <tr>
     <td>*客户名称:</td>
     <td><input type="text" class="insertSignCustName" readonly= "true " name="insertSignCustName" value="${receipt.custName}"/></td>
     <td>*商户名称:</td>
     <td><input type="text" class="insertSignPosCustName" readonly= "true " name="insertSignPosCustName" value="${receipt.posCustName}"/></td>
     </tr>
     <tr>
     <td>*贷款金额:</td>
     <td><input type="text" class="insertSignLoanAmount" readonly= "true " name="insertSignLoanAmount" value="${receipt.payApplyAmt}"/></td>
     <td>*贷款利率:</td>
     <td><input type="text" class="insertSignLoanInterest" readonly= "true " name="insertSignLoanInterest" value="${receipt.loanInterest}"/></td>
     </tr>
     <tr>
     <td>*起贷日:</td>
     <td><input type="text" class="insertSignBeginDate" readonly= "true " name="insertSignBeginDate" value="${receipt.beginDate}"/></td>
     <td>*到期日:</td>
     <td><input type="text" class="insertSignEndDate" readonly= "true " name="insertSignEndDate" value="${receipt.endDate}"/></td>
     </tr>
     <tr>
     <td>还款方式:</td>
     <td><input type="text" class="insertSignPaybackWay" readonly= "true " name="insertSignPaybackWay" value="${receipt.paybackWay}"/></td>
     <td>*贷款用途:</td>
     <td><input type="text" class="insertSignLoanUsage" readonly= "true " name="insertSignLoanUsage" value="${receipt.loanUsageName}"/></td>
     </tr>
     <tr>
     <td>银行账号:</td>
     <td><input type="text" class="insertSignPaybackAccount" readonly= "true " name="insertSignPaybackAccount" value="${receipt.loanAcNo}"/></td>
     <td>*账户开户行:</td>
     <td><input type="text" class="insertSignAccountOpenBank" readonly= "true " name="insertSignAccountOpenBank" value="${receipt.accountOpenBank}"/></td>
     </tr>
     <tr>
     <td>*账户分行:</td>
     <td><input type="text" class="insertSignAccountBranchBank" readonly= "true " name="insertSignAccountBranchBank" value="${receipt.accountBranckBank}"/></td>
     <td>*账户支行:</td>
     <td><input type="text" class="insertSignAccountSubBranchBank" readonly= "true " name="insertSignAccountSubBranchBank" value="${receipt.accountSubBranchBank}"/></td>
     </tr>
     <tr>
     <td>*贷款总余额:</td>
     <td><input type="text" class="insertSignLoanTotalBalance" readonly= "true " name="insertSignLoanTotalBalance" value="${receipt.loanTotalBalance}"/></td>
     <td>*正常余额:</td>
     <td><input type="text" class="insertSignNormalBalance" readonly= "true " name="insertSignNormalBalance" value="${receipt.normalBalance}"/></td>
     </tr>
     <tr>
     <td>*逾期余额:</td>
     <td><input type="text" class="insertSignOverdueBalance" readonly= "true " name="insertSignOverdueBalance" value="${receipt.overdueBalance}"/></td>
     <td>*表内欠息:</td>
     <td><input type="text" class="insertSignInnerOwnedInterest" readonly= "true " name="insertSignInnerOwnedInterest" value="${receipt.innerOwnedInterest}"/></td>
     </tr>
     <tr>
     <td>*表外 欠息:</td>
     <td><input type="text" class="insertSignOutterOwnedInterest" readonly= "true " name="insertSignOutterOwnedInterest" value="${receipt.outterOwnedInterest}"/></td>
     </tr>
     </table>
     </div>