package com.hrbb.loan.pos.util.exception;

public class BaseBusinessException extends Exception{

    /**  */
    private static final long serialVersionUID = -1971522612963693631L;

    public BaseBusinessException(){
        
    }
    
    public BaseBusinessException(String respCode,String msg){
        this.respCode=respCode;
        this.msg=msg;
    }
    
    private String respCode;

    private String msg;

    public String getRespCode() {
        return respCode;
    }

    public void setRespCode(String respCode) {
        this.respCode = respCode;
    }

    public String getMsg() {
        return msg;
    }

    public void setMsg(String msg) {
        this.msg = msg;
    }
    
    
}
