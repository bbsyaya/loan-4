        name="prodId" <c:if test="${oprFlag=='3'}">readonly="readonly"</c:if>
        value="${tApprovingAmountRateUplimitConfig.prodId}" size="30">
         --%>
       <select id="prodId" name="prodId" 
								class="easyui-combobox" validType="selectedRequired" required=true editable=false>
									<option value="">--请选择产品代码--</option>
									<c:forEach items="${productList}" var="obj">
										<option value="${obj.itemNo}" <c:if test="${obj.itemNo == tApprovingAmountRateUplimitConfig.prodId}">selected</c:if>>${obj.itemName}</option>
									</c:forEach>
		</select>
        </td>
      <td class="tdtitle">渠道风险类型:</td>
      <td>
     <%--  <input type="text" class="channel_rtype_code"
        name="channel_rtype_code"
        <c:if test="${oprFlag=='3'}">readonly="readonly"</c:if>
        value="${tApprovingAmountRateUplimitConfig.channel_rtype_code}"
        size="30"> --%>
        
        <select id="channel_rtype_code" name="channel_rtype_code" 
