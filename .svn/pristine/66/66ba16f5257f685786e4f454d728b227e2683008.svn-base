									<option value="">--请选择渠道风险类型--</option>
									<c:forEach items="${channelrisktypeList}" var="obj">
										<option value="${obj.itemNo}" <c:if test="${obj.itemNo == tApprovingAmountRateUplimitConfig.channel_rtype_code}">selected</c:if>>${obj.itemName}</option>
									</c:forEach>
		</select>
        </td>
    </tr>
    <tr>
      <td class="tdtitle">地区风险类型:</td>
