									<option value="">--请选择地区风险类型--</option>
									<c:forEach items="${regionrisktypeList}" var="obj">
										<option value="${obj.itemNo}" <c:if test="${obj.itemNo == tApprovingAmountRateUplimitConfig.region_rtype_code}">selected</c:if>>${obj.itemName}</option>
									</c:forEach>
		</select>
