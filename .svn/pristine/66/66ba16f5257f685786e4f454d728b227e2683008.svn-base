        <c:if test="${oprFlag=='3'}">readonly="readonly"</c:if>
        value="${tApprovingAmountRateUplimitConfig.region_rtype_code}" size="30"> --%>
        
      <select id="region_rtype_code" name="region_rtype_code" 
