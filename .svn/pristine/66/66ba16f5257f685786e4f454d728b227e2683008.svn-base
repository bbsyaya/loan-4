      <td class="tdtitle">还款方式:</td>
      <td>
      <%-- <input type="text" class="repay_type" name="repay_type"
        <c:if test="${oprFlag=='3'}">readonly="readonly"</c:if>
        value="${tApprovingAmountRateUplimitConfig.repay_type}" size="30"> --%>
        
        <select id="repay_type" name="repay_type" 
								class="easyui-combobox" validType="selectedRequired" required=true editable=false>
									<option value="">--请选择还款方式--</option>
									<c:forEach items="${paybackwayList}" var="obj">
										<option value="${obj.itemNo}" <c:if test="${obj.itemNo == tApprovingAmountRateUplimitConfig.repay_type}">selected</c:if>>${obj.itemName}</option>
									</c:forEach>
		</select>
        </td>
    </tr>
    <tr>
      <td class="tdtitle">还款期限:</td>
      <td><input type="text" class="maturity" name="maturity"
        <c:if test="${oprFlag=='3'}">readonly="readonly"</c:if>
        value="${tApprovingAmountRateUplimitConfig.maturity}" size="30" /></td>
      <td class="tdtitle">是否生效:</td>
      <td>
      <%-- <select class="deleted_flag" style="width: 243px;" name = "deleted_flag"
        <c:if test="${oprFlag=='3'}">disabled</c:if>>
          <option value="Y">是</option>
          <option <c:if test="${tApprovingAmountRateUplimitConfig.deleted_flag == 'N'}">selected</c:if> value="N">否</option>
      </select> --%>
      
      <select id="deleted_flag" name="deleted_flag" 
								class="easyui-combobox" validType="selectedRequired" required=true editable=false>
									<option value="">--请选择是否有效--</option>
									<c:forEach items="${deleteflagList}" var="obj">
										<option value="${obj.itemNo}" <c:if test="${obj.itemNo == tApprovingAmountRateUplimitConfig.deleted_flag}">selected</c:if>>${obj.itemName}</option>
									</c:forEach>
	  </select>
      </td>
    </tr>
    <tr>
      <td class="tdtitle">生效时间:</td>
      <td><input type="text" class="valid_date easyui-datebox"
        name="valid_date"
        <c:if test="${oprFlag=='3'}">readonly="readonly"</c:if>
        value="<fmt:formatDate value='${tApprovingAmountRateUplimitConfig.valid_date}' pattern='yyyy-MM-dd'/>"
        size="30" /></td>
      <td class="tdtitle">失效时间:</td>
      <td><input type="text" class="invalid_date easyui-datebox"
        name="invalid_date"
        <c:if test="${oprFlag=='3'}">readonly="readonly"</c:if>
        value='<fmt:formatDate value="${tApprovingAmountRateUplimitConfig.invalid_date}" pattern="yyyy-MM-dd"/>'
        size="30"></td>
