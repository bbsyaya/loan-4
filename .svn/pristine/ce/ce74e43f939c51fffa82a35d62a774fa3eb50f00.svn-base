package com.hrbb.loan.pos.facade.abs;

import java.io.Serializable;

/**
 * 
 * @author XLY
 * @version $Id: AbstractResponse.java, v 0.1 2015-2-16 下午1:48:10 XLY Exp $
 */
public class AbstractResponse implements Serializable {

    /**  */
    private static final long serialVersionUID = 2686762852838527861L;
    
    /**返回码*/
    private String respCode;
    
    private String memo;

    public String getRespCode() {
        return respCode;
    }

    public void setRespCode(String respCode) {
        this.respCode = respCode;
    }

    public String getMemo() {
        return memo;
    }

    public void setMemo(String memo) {
        this.memo = memo;
    }

    @Override
    public String toString() {
        return "AbstractResponse [respCode=" + respCode + ", memo=" + memo + "]";
    }
    
}
