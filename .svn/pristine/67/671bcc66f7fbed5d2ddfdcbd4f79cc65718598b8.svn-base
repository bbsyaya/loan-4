<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
	<!--修改窗口-->
  <!--修改窗口-->
    <div id="modifyCustMerchantWindow" class="easyui-window" title="新增授信申请" collapsible="false" minimizable="false"
        maximizable="true" icon="icon-save"  style="width: 800px; height: 500px; padding: 5px;
        background: #fafafa;">
        <div class="easyui-layout" fit="true">
            <div region="center" class="easyui-tabs" border="false" style="padding: 10px; background: #fff; border: 1px solid #ccc;">
                <div title="申请人个人信息" style="padding:20px;">
                	<table>
						<tr>
                			<td>		
                				商户名称:<input id="editposCustName" name="editposCustName" style="line-height:20px;border:1px solid  #ccc"/>
							</td>
                		    <td>		
                				经营区划：<input id="editbusiDivision" name="editbusiDivision" style="line-height:20px;border:1px solid #ccc"/>
							</td>
                		    <td>		
                				登记人名称：<input id="editoperName" name="editoperNamee" style="line-height:20px;border:1px solid #ccc"/>
							</td>
                		    <td>		
                				主营业务：<input id="editposCustBusiScope" name="editposCustBusiScope" style="line-height:20px;border:1px solid #ccc"/>
							</td>
 						<tr/>
 						<tr>
                			<td>		
                				地址详情:<input id="editposCustAddress" name="editposCustAddress" style="line-height:20px;border:1px solid #ccc"/>
							</td>
                		    <td>		
                				区划国标码：<input id="editoperAddrCode" name="editoperAddrCode" style="line-height:20px;border:1px solid #ccc"/>
							</td>
                		    <td>		
                				互金行业分类(国标)：<input id="editindustryTypeId" name="editindustryTypeId" style="line-height:20px;border:1px solid #ccc"/>
							</td>
                		    <td>		
                				互金行业分类(内部)：<input id="editindustryTypeId2" name="editindustryTypeId2" style="line-height:20px;border:1px solid #ccc"/>
							</td>
                		    <td>		
                				营业执照：<input id="editregiCode" name="editregiCode" style="line-height:20px;border:1px solid #ccc"/>
							</td>
 						<tr/>
 						<td><input type="hidden" id="posCustId" name="posCustId" readonly="readonly"></td>
                	</table>
                </div>
              
            </div>
            <div region="south" border="false" style="text-align: right; height: 30px; line-height: 30px;">
                <a id="btnEp" onclick="modifyCustMerchant()" class="easyui-linkbutton" icon="icon-ok" href="javascript:void(0)">
             		      保存并退出</a> <a id="btnCancel" class="easyui-linkbutton"  onclick="closeModifyCustMerchant()" icon="icon-cancel" href="javascript:void(0)">取消</a>
            </div>
        </div>
    </div>