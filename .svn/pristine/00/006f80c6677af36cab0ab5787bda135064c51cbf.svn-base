					var obj = eval('(' +data+')');
					//审批意见tab
					$('#modeAmount').val(obj.result08);
					$('#modeRate').val(obj.result09);
					$('#riskLevel').val(obj.result12);
					$('#modeWarn').val(obj.result07);
				}
