		//提交退回补件
		function addInfoSubmit() {
			if(!checkNeed()){
				return;
			}
		    var reqUrl = "<%=request.getContextPath()%>/addInfo/addInfoSubmit.do";
		    var rows = $('#tAddInfo').datagrid('getSelections');
		    var length = rows.length;
		    var addInfoCodes = '';
			for(var i=0; i<length; i++){
				if (addInfoCodes == ''){
					addInfoCodes=rows[i].itemNo;
				}else{
					addInfoCodes=addInfoCodes+"|"+rows[i].itemNo;
				}
			}
			var row = $('#t').datagrid('getSelected');
		    var apprAmount = "";
		    if($('#apprAmount').length > 0){
		    	apprAmount =getNumberValue('#apprAmount');
		    }
			var aie = $("input[name='apprInfoExtC']:checked").map(function () {
	            return $(this).val();
	        }).get().join("|");
			
		    $.messager.progress({
	 	        text: '数据处理中，请稍等......',
	 	    }); 
		    
			$.post(reqUrl, 
				{loanid: row.loanId,
				 applyStatus: row.applyStatus,
				 appNum: $('#appNum').val(),
				 addInfoCodes: addInfoCodes,
				 apprAmount: apprAmount,
				 apprInte: $('#apprInte').val(),
				 apprInfo: $('#apprInfo').val(),
				 //apprInfoExt: $('#apprInfoExt').val(),
				 apprInfoExt: aie,
				 remark: $('#remark').val(),
				 needReason: $('#needReason').val()
				}, 
				function(data){
					$.messager.progress('close');
					alert(data);
					closeAddInfoWindow();
					closeSignSubmitWindow();
					queryCreditApply();
				}
			);
		}
		
		//获取银行流水月份
		function getOverMonths(){
			var startMonth = $('#startMonth').val();
			var endMonth = $('#endMonth').val();
			var reqUrl = "<%=request.getContextPath()%>/creditApply/getSerialMonth.do";
			$.post(
				reqUrl, {startMonth :startMonth, endMonth :endMonth}, function(data){
				$('input.easyui-validatebox').validatebox('disableValidation');
				for(var i=0; i<=11; i++){
					$('#currMonth'+i).val('');
					$('#'+i).hide();
				}			
				var obj = eval("("+data+")");
				for(var i=0; i<obj.length; i++){
					$('#currMonth'+i).val(obj[i].month);
					$('#'+i).show();
				}
			});
		}
