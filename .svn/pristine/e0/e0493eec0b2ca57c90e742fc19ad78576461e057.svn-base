package com.hrbb.loan.pos.service.bean;

import java.io.Serializable;

/**
 * 影像文件缺失通知：影像资料文件包名
 * 
 * @author guoyu
 * @version $Id: ImageFileLostMessage.java, v 0.1 2015年3月16日 下午6:28:39 guoyu Exp
 *          $
 */
public class ImageFileLostMessage implements Serializable {

	/**  */
	private static final long serialVersionUID = -4231936380935703026L;

	private String imagefilepackagename;

	/**
	 * Getter method for property <tt>imagefilepackagename</tt>.
	 * 
	 * @return property value of imagefilepackagename
	 */
	public String getImagefilepackagename() {
		return imagefilepackagename;
	}

	/**
	 * Setter method for property <tt>imagefilepackagename</tt>.
	 * 
	 * @param imagefilepackagename
	 *            value to be assigned to property imagefilepackagename
	 */
	public void setImagefilepackagename(String imagefilepackagename) {
		this.imagefilepackagename = imagefilepackagename;
	}

	public String toString() {
		return new String("\"imagefilepackagename\":\"" + imagefilepackagename + "\"").replaceAll("[\\t\\n\\r]", "");
	}
}
